    <connection>cadastro</connection>
    <sql>SELECT dp.nome_completo, dp.matricula, sc.data_inicio_afastamento, sc.data_termino_afastamento, t.mes, 
'Servidor '||SUBSTRING(SUBSTRING(CAST (RANDOM() AS VARCHAR(100)) FROM 1 FOR 10),3,10 ) AS nome_mascarado,
TO_DATE(data_termino_afastamento, 'dd/mm/yyyy') -  TO_DATE(data_inicio_afastamento, 'dd/mm/yyyy') AS tempo_afastamento
,SUBSTRING(sc.nome, '1', POSITION(' ' IN sc.nome)) AS primeiro_nome
FROM stage.cadastro_tratada AS sc
INNER JOIN stage.remuneracao_tratada AS r
ON sc.nome = r.nome
INNER JOIN dw_servidor.dm_dados_pessoais AS dp
ON sc.nome = dp.nome_completo 
AND sc.cpf = dp.cpf
AND sc.matricula = dp.matricula
INNER JOIN dw_servidor.dm_tempo AS t
ON r.mes = r.mes</sql>
